<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="baseline.ToDoMainController">
   <items>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="47.0" prefWidth="520.0">
         <children>
            <Button fx:id="NewListButton" layoutX="14.0" layoutY="10.0" mnemonicParsing="false" onAction="#addNewList" text="New List" />
            <Button fx:id="RemoveListButton" layoutX="287.0" layoutY="10.0" mnemonicParsing="false" onAction="#removeList" text="Remove List" />
            <Button fx:id="EditListButton" layoutX="375.0" layoutY="10.0" mnemonicParsing="false" onAction="#editList" text="Edit List" />
            <TextField layoutX="75.0" layoutY="10.0" prefHeight="25.0" prefWidth="205.0" />
            <Button fx:id="SaveListButton" layoutX="444.0" layoutY="10.0" mnemonicParsing="false" onAction="#saveList" text="Save List" />
         </children>
      </AnchorPane>
      <ListView fx:id="ToDoListsView" prefHeight="200.0" prefWidth="200.0" />
   </items>
</SplitPane>
